{
	"name": "astro-variables",
	"version": "0.0.1",
	"description": "A package for managing variables between server and client in Astro projects",
	"author": {
		"email": "varharrie@gmail.com",
		"name": "varHarrie",
		"url": "https://github.com/varharrie"
	},
	"license": "MIT",
	"keywords": [
		"astro-integration",
		"astro-component",
		"withastro",
		"astro",
		"variables"
	],
	"homepage": "https://github.com/varharrie/astro-variables#readme",
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"exports": {
